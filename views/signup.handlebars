<html>
	<head>
		<title>Watch Your Worth</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link rel="stylesheet" href="css/style.css">
		<link rel="stylesheet" href="css/signup.css">
	</head>
	<body>
		<nav>
			<div id="icon-container">
				<div id="icon-image"></div>
			</div>
			<div id="homepage-container">
				<a id="homepage" href="/">Back To Homepage</a>
			</div>
		</nav>
		<div id="sign-up-content-wrapper">
			<h2>Sign Up</h2>
			<form id="sign-up-form" method="POST" action="">
				<div id="sign-up-content-container">
					<div id="sign-up-content">
						<div class="sign-up-item sign-up-header">Username:</div>
						<div class="sign-up-item sign-up-header">Password:</div>
					</div>
					<div id="sign-up-content-input">
						<div class="sign-up-item sign-up-input"><input name="username" type="text"></div>
						<div class="sign-up-item sign-up-input"><input name="password" type="password"></div>
					</div>
				</div>
				<input type="submit" value="Sign Up">	
			</form>
		</div>
	</body>
</html>